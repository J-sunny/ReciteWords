<view class="infoBox"><view class="hedbox"><image class="site-img" src="../../../../static/images/ddd.png" catchtap="navmap"></image><view class="arrow" bindtap="__e" data-event-opts="{{[['tap',[['goBack',['$event']]]]]}}"></view><view class="title">我的资料</view><view class="touBox"><view data-event-opts="{{[['tap',[['show',['headPic']]]]]}}" class="touXiang" bindtap="__e"><image src="../../../../static/images/touXiang@2x.png"></image></view></view></view><view class="infoCon"><view class="conBox"><label class="name">昵称/姓名</label><label class="con">{{getUserInfoList.teacherAccount}}</label></view><view class="conBox"><label class="name">工号</label><label class="con">{{getUserInfoList.teacherNum}}</label></view><view data-event-opts="{{[['tap',[['show1',['gender']]]]]}}" class="conBox" bindtap="__e"><label class="name">性别</label><label class="con cons">{{teacherGender==0?'女':'男'}}</label><label data-event-opts="{{[['tap',[['show1',['gender']]]]]}}" class="arrowRight" bindtap="__e"></label></view><view data-event-opts="{{[['tap',[['show2',['']]]]]}}" class="conBox" bindtap="__e"><label class="name">生日</label><label class="con cons">{{teacherBirth}}</label><label data-event-opts="{{[['tap',[['show2',['']]]]]}}" class="arrowRight" bindtap="__e"></label></view><view class="conBox"><label class="name">学校</label><label class="con">{{getUserInfoList.teacherRealname}}</label></view><navigator class="conBox" url="changePassword"><label class="name">修改密码</label><navigator class="arrowRight" url="changePassword"></navigator></navigator></view><view class="headPicPopup"><popup-layer class="vue-ref" vue-id="1" direction="top" data-ref="headPic" bind:__l="__l" vue-slots="{{['default']}}"><view class="popupBox"><view class="selectBox"><view class="opcition">拍照</view><view>从手机相册选择</view></view><view data-event-opts="{{[['tap',[['close',['headPic']]]]]}}" class="cancel" bindtap="__e">取消</view></view></popup-layer></view><view class="genderPopup"><popup-layer class="vue-ref" vue-id="2" direction="top" data-ref="gender" bind:__l="__l" vue-slots="{{['default']}}"><view class="popupBox"><van-picker vue-id="{{('3')+','+('2')}}" show-toolbar title="性别" columns="{{columns}}" default-index="{{getUserInfoList.teacherGender}}" data-event-opts="{{[['^cancel',[['onCancel']]],['^confirm',[['onConfirm']]]]}}" data-com-type="wx" bind:cancel="__e" bind:confirm="__e" bind:__l="__l"></van-picker></view></popup-layer></view><view class="birthday"><popup-layer class="vue-ref" vue-id="4" direction="top" data-ref="birthday" bind:__l="__l" vue-slots="{{['default']}}"><van-datetime-picker vue-id="{{('5')+','+('4')}}" type="date" value="{{currentDate}}" min-date="0" formatter="{{formatter}}" max-date="{{maxDate}}" data-event-opts="{{[['^cancel',[['birCancel']]],['^input',[['birInput']]],['^confirm',[['birConfirm']]]]}}" data-com-type="wx" bind:cancel="__e" bind:input="__e" bind:confirm="__e" bind:__l="__l"></van-datetime-picker></popup-layer></view></view>