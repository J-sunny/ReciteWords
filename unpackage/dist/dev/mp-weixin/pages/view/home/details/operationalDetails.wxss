.missionDetailsBox {
  position: relative;
  overflow: hidden;
}
.missionDetailsBox .makeSureTxt {
    text-align: center;
    font-size: 323rpx;
    font-family: PingFang SC;
    font-weight: 400;
    line-height: 48rpx;
    color: #2e3548;
    opacity: 1;
}
.missionDetailsBox .headBox {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
}
.missionDetailsBox .site-img {
    width: 100%;
    position: absolute;
    left: 0;
    top: 0;
    height: 288rpx;
    z-index: 10;
}
.missionDetailsBox .arrow {
    position: fixed;
    left: 40rpx;
    top: 73rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    width: 20rpx;
    height: 20rpx;
    border-left: 4rpx solid #fff;
    border-top: 4rpx solid #fff;
    -webkit-transform: rotate(-45deg);
        -ms-transform: rotate(-45deg);
            transform: rotate(-45deg);
    z-index: 20;
}
.missionDetailsBox .title {
    width: 100%;
    text-align: center;
    position: absolute;
    left: 0;
    top: 60rpx;
    font-size: 36rpx;
    font-weight: 800;
    color: white;
    opacity: 1;
    z-index: 15;
}
.missionDetailsBox .taskVocabulary {
    position: absolute;
    top: 130rpx;
    width: 100%;
    height: 288rpx;
    background-image: url(data:image/png;base64,);
    z-index: 12;
    background-size: 100% 100%;
    padding: 86rpx 64rpx 0;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.missionDetailsBox .taskVocabulary .taskLeft {
      float: left;
}
.missionDetailsBox .taskVocabulary .taskLeft .taskTitle {
        font-size: 36rpx;
        font-weight: bold;
        color: #2e3548;
        opacity: 1;
}
.missionDetailsBox .taskVocabulary .taskLeft .taskTime {
        font-size: 24rpx;
        font-weight: 400;
        color: #979dab;
        opacity: 1;
        margin-top: 16rpx;
}
.missionDetailsBox .taskVocabulary .taskLeft .status {
        display: inline-block;
        font-size: 22rpx;
        font-family: PingFang SC;
        font-weight: 400;
        line-height: 44rpx;
        color: #979dab;
        opacity: 1;
        width: 90rpx;
        height: 44rpx;
        background: rgba(239, 239, 241, 0.5);
        border: 2rpx solid #c9c9c9;
        opacity: 1;
        border-radius: 4rpx;
        text-align: center;
        margin-left: 16rpx;
}
.missionDetailsBox .taskVocabulary .viewBtn {
      width: 140rpx;
      height: 56rpx;
      float: right;
      line-height: 56rpx;
      background: -webkit-gradient(linear, left top, right top, from(#fec91b), to(#ffbb00));
      background: -o-linear-gradient(left, #fec91b 0%, #ffbb00 100%);
      background: linear-gradient(90deg, #fec91b 0%, #ffbb00 100%);
      opacity: 1;
      border-radius: 40rpx;
      text-align: center;
      font-size: 24rpx;
      font-weight: 400;
      margin-top: 22rpx;
      color: white;
}
.missionDetailsBox .taskVocabulary .delectBtn {
      width: 140rpx;
      height: 56rpx;
      float: right;
      line-height: 56rpx;
      background: #FE661C;
      opacity: 1;
      border-radius: 40rpx;
      text-align: center;
      font-size: 24rpx;
      font-weight: 400;
      margin-top: 22rpx;
      color: white;
}
.missionDetailsBox .contentBox {
    padding: 0 32rpx 50rpx;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    margin-top: 400rpx;
}
.missionDetailsBox .contentBox .classMajor {
      margin-top: 390rpx;
      font-size: 30rpx;
      font-weight: bold;
      color: #2e3548;
}
.missionDetailsBox .contentBox .classMajor .major {
        font-size: 26rpx;
        font-weight: 400;
}
.missionDetailsBox .contentBox .frame {
      width: 100%;
      height: 160rpx;
      background: white;
      opacity: 1;
      border-radius: 16rpx;
      margin-top: 32rpx;
      padding: 0 32rpx;
      -webkit-box-sizing: border-box;
      box-sizing: border-box;
}
.missionDetailsBox .contentBox .frame .frameLeft {
        float: left;
}
.missionDetailsBox .contentBox .frame .frameLeft .stuName {
          font-size: 26rpx;
          font-family: PingFang SC;
          font-weight: 400;
          color: #2e3548;
          opacity: 1;
          margin-top: 30rpx;
}
.missionDetailsBox .contentBox .frame .frameLeft .stuName .stuNames {
            font-size: 26rpx;
            font-weight: 400;
            color: #979dab;
            opacity: 1;
}
.missionDetailsBox .contentBox .frame .frameLeft .stuName .completionStatus {
            font-size: 22rpx;
            font-weight: 400;
            width: 84rpx;
            height: 40rpx;
            border-radius: 2px;
            display: inline-block;
            text-align: center;
            line-height: 40rpx;
            margin-left: 24rpx;
}
.missionDetailsBox .contentBox .frame .frameLeft .stuName .completionStatus.do {
            color: #ffbb00;
            border: 1px solid #ffbb00;
}
.missionDetailsBox .contentBox .frame .frameLeft .stuName .completionStatus.notDo {
            border: 1px solid #c9c9c9;
            color: #C9C9C9;
}
.missionDetailsBox .contentBox .frame .frameRightBtn {
        float: right;
        font-size: 26rpx;
        font-weight: 400;
        color: #ffbb00;
        opacity: 1;
        margin-top: 62rpx;
}

